{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  // You can have "AzureAd" if you are using Microsoft Entra ID (Azure Active Directory - previously)
